{
    "rewrites": [
        { "source": "/(.*)", "destination": "/api/app" }
    ]
}

{
    "version": 2,
    "builds": [
        {
            "src": "./api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}

